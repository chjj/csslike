/**
 * HTML5 Elements
 */

header,
footer,
nav,
section,
aside,
article,
hgroup,
figure,
figcaption {
  display: block;
}

/**
 * Basic Elements
 */

::selection,
input::selection,
textarea::selection {
  background: rgba(0, 0, 0, 0.5);
  color: rgba(255, 255, 255, 0.8);
  text-shadow: none;
}
::-moz-selection,
input::-moz-selection,
textarea::-moz-selection {
  background: rgba(0, 0, 0, 0.5);
  color: rgba(255, 255, 255, 0.8);
  text-shadow: none;
}

/**
 * Text Level
 */

a,
h1 > a:hover {
  text-decoration: none;
}
a:focus {
  outline: none;
}
a:hover,
a:focus {
  text-decoration: underline;
}
abbr {
  border: none;
  text-decoration: none;
}
abbr[title] {
  cursor: help;
  border-bottom: rgba(0, 0, 0, 0.15) solid 1px;
}
p > code,
samp {
  background: rgba(255, 255, 255, 0.3);
  border: rgba(255, 255, 255, 0.3) solid 1px;
  text-shadow: none;
  font: 14/20px monospace;
}
mark {
  background: rgba(255, 255, 255, 0.3);
}
small {
  font-size: 11px;
}

/**
 * Content Flow
 */

p,
ul,
ol,
dl,
dd,
figure,
blockquote,
pre,
hr,
form {
  margin: 0 0 20px;
  padding: 0;
  word-wrap: break-word;
}

/**
 * Content Blocks
 */

figure,
blockquote,
pre {
  padding: 20px;
}
figure > *,
blockquote > *,
pre > * {
  padding: 0;
  background: transparent !important;
}
figcaption {
  margin: 20px 20px 0 20px;
}
blockquote {
  margin-left: 45px;
  padding-bottom: 1px;
  margin-top: -1px;
}
blockquote:before {
  content: "“";
  float: left;
  margin: -30px 0 0 -65px;
  font: 90px/1 serif;
}
blockquote > cite {
  display: block;
  margin: 20px 40px;
  font-style: italic;
}
pre {
  white-space: pre-wrap;
}
pre > code {
  font-size: 11px;
}
pre > code[title]:before {
  float: right;
  margin: -10px -5px 0 0;
  content: "[" attr(title) "]";
}
h1,
h2,
h3,
h4,
h5,
h6,
dt {
  font: normal 60px/20px serif;
  letter-spacing: 0.05em;
  padding: 0;
  margin: 0 0 20px;
}
h1 {
  line-height: 60px;
}
h2 {
  font-size: 26px;
}
h3,
dt {
  font-size: 22px;
}
h4 {
  font-size: 18px;
}
h5,
h6 {
  font-size: 16px;
}
hr {
  height: auto;
  border: none;
  font: italic 60px/50px serif;
  text-align: center;
  cursor: default;
  margin: 0 auto 10px;
  width: 30px;
  height: 60px;
  -webkit-transform: rotate(71deg);
  -moz-transform: rotate(71deg);
  -o-transform: rotate(71deg);
  transform: rotate(71deg);
}
hr:before {
  content: "§";
}
img {
  display: block;
  outline: none;
  border: solid 1px;
  opacity: 0.6;
  -webkit-transition: 0.25s;
  -moz-transition: 0.25s;
  -o-transition: 0.25s;
  transition: 0.25s;
}
:hover > img,
img:hover {
  opacity: 1;
}
p > img {
  float: right;
  margin: 0 0 20px 20px;
}
p:nth-of-type(even) > img {
  float: left;
  margin: 0 20px 20px 0;
}

/**
 * Form Controls
 */

input,
textarea {
  display: block;
  height: 20px;
  font: inherit;
  text-shadow: inherit;
  margin: 0;
  padding: 0 2px;
  -webkit-box-sizing: border-box;
  -moz-box-sizing: border-box;
  box-sizing: border-box;
}

/**
