/**
 * Media Queries
 */

@media print {
  html {
    width: auto;
    padding: 0;
  }
  body {
    margin: 0;
    padding: 0;
  }
  body:before,
  body:after,
  body > header,
  footer,
  h1 + time,
  form {
    display: none;
  }
  h1,
  h2,
  h3,
  h4,
  h5,
  h6 {
    page-break-after: avoid;
  }
  pre,
  blockquote,
  figure,
  aside[role="note"] {
    page-break-inside: avoid;
  }
}


/**
 * Adjustment
 */

@media (max-width: 840px) {

  /**
   * change design into a fluid layout
   */

  @viewport  {
    width: device-width;
    zoom: 1.0;

    /**
     * user-zoom: fixed;
     */

  }

  html,
  body > header > * {
    width: auto;
  }
  header + * > footer {
    left: auto;
    margin-left: 0;
    right: 0;
  }
  h1 + time {
    margin-right: 0;
    right: 100px;
  }
  body > footer {
    right: 60px;
    margin-right: 5px;
  }
}

@media (max-width: 760px) {
  nav li:nth-child(3),
  form > p {
    display: none;
  }
  article form {
    overflow: hidden;
  }
  label {
    font-size: 9px;
    border: none;
    padding: 0;
  }
}

@media (max-width: 690px) {
  label > input {
    width: 50%;
  }
}


/**
 * at this point, its safe to assume the person is probably using a mobile device that doesnt support position: fixed
 */

@media (max-width: 600px) {
  html {
    overflow: auto;
    padding-bottom: 40px;
    background-image: none;
  }
  body {
    margin: 0;
    padding: 20px;
    position: relative;
    border: none;
    overflow: hidden;
  }
  a[rel~=home] {
    margin-left: 10px;
  }
  header + * h1 {
    padding-top: 20px;
    margin-left: 0;
    margin-right: 0;
  }
  body > footer {
    right: 5px;
  }
  body > header + * > footer {
    height: auto;
    width: auto;
    top: 30px;
    margin: 0 10px 0 0;
  }
  body > header + * > footer > a[href] {
    position: static;
    text-shadow: none;
    float: left;
    margin: 0 0 0 20px;
    width: auto;
    height: auto;
    font-size: 11px;
  }
  body > header + * > footer > a:before {
    display: none;
  }
  nav ul {
    margin-left: 60px;
  }
  head {
    display: none;
  }
  body > header,
  body > footer,
  body > header + * > footer {
    position: absolute;
  }
}

@media (max-width: 460px) {
  nav li:nth-child(2) {
    display: none;
  }
  footer > small {
    letter-spacing: 0;
  }
  label > input {
    width: 35%;
  }
}

@media (max-width: 320px) {
  footer > small {
    font-size: 8px;
  }
  nav li:last-child {
    display: none;
  }
}


