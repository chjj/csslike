     */

    margin-right: 90px;
    z-index: -1;
    opacity: 0;
    pointer-events: none;
    -webkit-transition: 0.4s;
    -moz-transition: 0.4s;
    -o-transition: 0.4s;
    transition: 0.4s;
    border: solid 1px;
  }
  footer > form:target {
    opacity: 1;
    pointer-events: auto;
  }
  footer > form:before {
    display: block;
    position: absolute;
    top: 0;
    content: "";
    background: rgba(255, 255, 255, 0.3);
    height: 20px;
    width: 15px;
    border-right: rgba(170, 0, 0, 0.3) solid 15px;
    background: rgba(255, 255, 255, 0.2);
    border-right: none;
    width: 20px;
  }
  footer input[type="search"] {
    background: #fff;
    border: black solid 1px;
    -webkit-appearance: none;
    height: 20px;
    width: 140px;
    float: left;
    padding: 2px;
    font-size: 12px;
    line-height: 1;
    border: none;
    background: rgba(255, 255, 255, 0.9);
  }
  footer input[type="submit"] {
    height: 20px;
    padding: 0;
    position: absolute;
    top: 2px;
    background: transparent;
    border: none;
    text-transform: lowercase;
    cursor: pointer;
  }

